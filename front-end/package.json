  "dependencies": {
    "@datagridxl/datagridxl2": "^2.0.10",
    "@imengyu/vue3-context-menu": "git+https://git@github.com/laorange/vue3-context-menu.git",
    "axios": "^0.27.2",
    "dayjs": "^1.11.3",
    "pinia": "^2.0.14",
    "pocketbase": "^0.2.1",
    "vant": "^3.5.2",
    "vue": "^3.2.25",
    "vue-clipboard3": "^2.0.0",
    "vue-router": "^4.1.1",
    "vue3-storage": "^0.1.11"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "naive-ui": "^2.30.8",
    "typescript": "^4.5.4",
    "unplugin-vue-components": "^0.21.0",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.7"
