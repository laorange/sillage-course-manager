  "version": "0.7.3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laorange/sillage-course-manager.git"
  },
  "bugs": {
    "url": "https://github.com/laorange/sillage-course-manager/issues"
  },
  "homepage": "https://github.com/laorange/sillage-course-manager#readme",
  "scripts": {
    "dev": "vite serve --mode development",
    "production": "vite serve --mode production",
    "release": "vite serve --mode release",
    "build:dev": "vue-tsc --noEmit && vite build --base=./ --mode development",
    "build:production": "vue-tsc --noEmit && vite build --base=./ --mode production",
    "build:go": "vue-tsc --noEmit && vite build --outDir ../back-end/dist --base=./ --mode go",
    "build:go_release": "vue-tsc --noEmit && vite build --outDir ../back-end/dist --base=./ --mode release",
    "delete:dist": "del \"../back-end/dist/assets/\"",
    "preview": "vite preview"
  },
  "dependencies": {
    "@datagridxl/datagridxl2": "^2.0.10",
    "@imengyu/vue3-context-menu": "git+https://git@github.com/laorange/vue3-context-menu.git",
    "axios": "^0.27.2",
    "dayjs": "^1.11.3",
    "pinia": "^2.0.14",
    "pocketbase": "^0.2.1",
    "vant": "^3.5.4",
    "vue": "^3.2.25",
    "vue-clipboard3": "^2.0.0",
    "vue-router": "^4.1.1",
    "vue3-storage": "^0.1.11"
  },
  "devDependencies": {
    "@vicons/ionicons5": "^0.12.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "naive-ui": "^2.32.1",
    "typescript": "^4.5.4",
    "unplugin-vue-components": "^0.21.0",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.7"
  }
}
